{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "s3:*",
		"Resource": [ "arn:aws:s3:::store-prod.split4ever.com","arn:aws:s3:::store-dev.split4ever.com" ]
        }
        , {
            "Effect": "Allow",
            "Action": [
                "dynamodb:DescribeStream",
                "dynamodb:GetRecords",
                "dynamodb:GetShardIterator",
                "dynamodb:ListStreams",
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
		"Resource": [ "arn:aws:dynamodb:us-east-1:692391178777:table/part-dev","" ]
        }
    ]
}